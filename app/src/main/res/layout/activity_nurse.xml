<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".daktari2.Nurse">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:src="@drawable/singin_top"
        app:layout_constraintTop_toTopOf="parent"/>


    <ImageView
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:src="#0099CC"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:gravity="left"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="60dp"
        android:layout_marginStart="20dp"
        android:text="Ms. Nurse"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        android:fontFamily="sans-serif"
        tools:targetApi="s" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:layout_marginTop="50dp"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="96dp"
        app:srcCompat="@drawable/baseline_notifications_24" />



    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:layout_marginTop="50dp"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="20dp"
        app:srcCompat="@drawable/baseline_message_24"/>

    <EditText
        android:id="@+id/editText"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginTop="130dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:padding="5dp"
        android:hint="Search Articles and News"
        android:textSize="11sp"
        android:textColor="@color/black"
        android:fontFamily="sans-serif"
        android:background="@drawable/edittext">
    </EditText>

    <Button
        android:id="@+id/btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:shapeAppearanceOverlay="@style/Button5"
        android:backgroundTint="#21005D"
        android:text="Search"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:fontFamily="sans-serif"
        android:padding="10dp"
        android:layout_marginTop="136dp"
        android:layout_marginEnd="5dp"
        android:layout_alignEnd="@id/editText"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/btn1"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="200dp"
        android:layout_marginStart="14dp"
        android:text="My Sessions"
        android:textSize="18sp"
        android:textColor="@color/white"
        app:shapeAppearanceOverlay="@style/Button3"
        android:backgroundTint="#21005D" />


    <Button
        android:id="@+id/btn3"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="200dp"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="14dp"
        android:text="On Demand Medic"
        android:textSize="18sp"
        android:textColor="@color/white"
        app:shapeAppearanceOverlay="@style/Button3"
        android:backgroundTint="#21005D" />

    <Button
        android:id="@+id/btn4"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="400dp"
        android:layout_marginStart="14dp"
        android:text="My Shifts"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:shapeAppearanceOverlay="@style/Button3"
        android:backgroundTint="#21005D" />


    <Button
        android:id="@+id/btn6"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="400dp"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="14dp"
        android:text="Help/FAQs"
        android:textColor="@color/white"
        android:textSize="17sp"
        app:shapeAppearanceOverlay="@style/Button3"
        android:backgroundTint="#21005D" />


    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/addContinue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#0099CC"
        android:contextClickable="true"
        android:clickable="true"
        android:layout_marginBottom="90dp"
        android:layout_marginEnd="45dp"
        android:contentDescription="TODO"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        app:srcCompat="@drawable/baseline_add_24"
        android:focusable="true" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="0dp"
        android:layout_alignParentBottom="true"
        android:background="#2971C4"
        app:menu="@menu/bottom_nav">
    </com.google.android.material.bottomnavigation.BottomNavigationView>




</RelativeLayout>